<UserControl x:Class="AutomationModernUI.Pages.ViewResults.ResultsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab  Name="MainList" Layout="List" SelectedSource="/Pages/ViewResults/LastRunPage.xaml">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="My Last Run" Source="/Pages/ViewResults/LastRunPage.xaml"/>
                <mui:Link DisplayName="Last Run" Source="/Pages/ViewResults/MyLastRunPage.xaml"/>
                <!--<mui:Link DisplayName="Statistics" Source="/Pages/ViewResults/StatisticsPage.xaml"/>-->
                <!--<mui:Link DisplayName="Downloads" Source="/Pages/ViewResults/DownloadsPage.xaml"/>-->
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
